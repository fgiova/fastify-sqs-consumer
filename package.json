{
	"name": "@fgiova/fastify-sqs-consumer",
	"description": "sqs-consumer fastify plugin",
	"version": "1.1.0",
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"lint": "eslint 'src/**/*.ts'",
		"clean": "rimraf dist",
		"build": "tsc",
		"test": "tap -t90 --before=test/scripts/executors/before.js --after=test/scripts/executors/after.js",
		"test:local": "TEST_LOCAL=true tap -t90",
		"test:debug": "tap -T -O --before=test/scripts/executors/before.js --after=test/scripts/executors/after.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fgiova/fastify-sqs-consumer.git"
	},
	"author": "Francesco Giovannini <fgiova@fgiova.com>",
	"license": "MIT",
	"keywords": [
		"fastify",
		"sqs",
		"sqs-consumer",
		"consumer",
		"aws",
		"amazon"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"tap": {
		"node-arg": "--require=ts-node/register",
		"ts": true,
		"lines": 100,
		"branches": 100,
		"statements": 100,
		"functions": 100
	},
	"devDependencies": {
		"@aws-sdk/client-sqs": "^3.398.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@types/node": "^18.17.1",
		"@types/tap": "^15.0.5",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"conventional-changelog-conventionalcommits": "^6.1.0",
		"cross-env": "^7.0.3",
		"dotenv": "^10.0.0",
		"eslint": "^8.1.0",
		"eslint-plugin-import": "^2.25.2",
		"fastify": "^4.21.0",
		"nodemon": "^2.0.14",
		"semantic-release": "^21.1.0",
		"tap": "^16.3.8",
		"testcontainers": "^10.2.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
	},
	"dependencies": {
		"fastify-plugin": "^4.5.1",
		"sqs-consumer": "^7.2.2"
	}
}
